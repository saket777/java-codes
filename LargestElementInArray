package com.company;

public class LargestElementInAnArray {
    public static void main(String[] args) {
        int arr[] = new int[]{2, 4, 6, 78, 98};
        int max = arr[0];
        for (int i = 0; i <= arr.length; i++) {
            if (arr[i] > max)
                max = arr[i];
        }
        System.out.println("Largest element in an array:" + max);
    }
}
