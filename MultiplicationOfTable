package com.company;

import java.util.Scanner;

public class MultiplicationTable {
    public static void main(String[] args) {
        int num;
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the number:");
        num = sc.nextInt();
        for(int i =0;i<=10;i++){
            System.out.printf("%d * %d = %d \n",num, i, num*i);
        }

    }
}
